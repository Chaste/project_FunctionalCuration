<?xml version="1.0" encoding="utf-8"?>
<sedML  level="1" version="1" xmlns="http://sed-ml.org/" xmlns:math="http://www.w3.org/1998/Math/MathML" xmlns:cellml="http://www.cellml.org/cellml/1.0#" xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
  <notes><p xmlns="http://www.w3.org/1999/xhtml">Comparing Limit Cycles and strange attractors for oscillation in Drosophila</p></notes> 
  <listOfSimulations>
    <uniformTimeCourse id="simulation1"
     initialTime="0" outputStartTime="0" outputEndTime="180" numberOfPoints="1000" >
      <algorithm kisaoID="KISAO:0000019"/>
     </uniformTimeCourse>
  </listOfSimulations>
  <listOfModels>
    <model id="model1" name="Circadian Oscillations" language="urn:sedml:language:cellml" source="leloup_gonze_goldbeter_1999_a.cellml" >
      <!--<listOfChanges>
        <changeAttribute id="c1" target="/cellml:model/cellml:component[@cmeta:id='MP']/cellml:variable[@name='vsP']/@initial_value" newValue="1"/>
        <changeAttribute id="c2" target="/cellml:model/cellml:component[@cmeta:id='MP']/cellml:variable[@name='vmP']/@initial_value" newValue="0.7"/>
        <changeAttribute id="c3" target="/cellml:model/cellml:component[@cmeta:id='P2']/cellml:variable[@name='vdP']/@initial_value" newValue="2"/>
        <changeAttribute id="c4" target="/cellml:model/cellml:component[@cmeta:id='T2']/cellml:variable[@name='vdT']/@initial_value" newValue="2"/>  
        <changeAttribute id="c5" target="/cellml:model/cellml:component[@name='parameters']/cellml:variable[@name='k1']/@initial_value" newValue="0.6"/>
        <changeAttribute id="c6" target="/cellml:model/cellml:component[@name='parameters']/cellml:variable[@name='K4P']/@initial_value" newValue="1"/>
        <changeAttribute id="c7" target="/cellml:model/cellml:component[@name='parameters']/cellml:variable[@name='K4T']/@initial_value" newValue="1"/>
      </listOfChanges>-->
    </model>
    <model id="model2" name="Circadian Chaos" language="urn:sedml:language:cellml" source="model1">
      <listOfChanges>
        <changeAttribute id="c8" target="/cellml:model/cellml:component[@cmeta:id='MT']/cellml:variable[@name='vmT']/@initial_value" newValue="0.28"/>
        <changeAttribute id="c9" target="/cellml:model/cellml:component[@cmeta:id='T2']/cellml:variable[@name='vdT']/@initial_value" newValue="4.8"/>        
      </listOfChanges>
    </model>
  </listOfModels>
 
  <listOfTasks>
    <task id="task1" name="Limit Cycle" modelReference="model1" simulationReference="simulation1"/>
    <task id="task2" name="Strange attractors" modelReference="model2" simulationReference="simulation1"/>
  </listOfTasks>
  <listOfDataGenerators>
    <dataGenerator id="time" name="time">
      <listOfVariables>
        <variable id="t" taskReference="task1" target="/cellml:model/cellml:component[@name='environment']/cellml:variable[@name='time']" />
      </listOfVariables>
       <math:math>
          <math:ci>t</math:ci>
        </math:math>
    </dataGenerator>
    
    <dataGenerator id="tim1" name="tim mRNA">
      <listOfVariables>
        <variable id="v4" taskReference="task1" target="/cellml:model/cellml:component[@cmeta:id='MT']/cellml:variable[@cmeta:id='MT_MT']" />
      </listOfVariables>
       <math:math>
          <math:apply>
            <math:plus />
            <math:ci>v4</math:ci>
          </math:apply>
        </math:math>
    </dataGenerator>
    
    <dataGenerator id="tim1_norm" name="tim mRNA (normalised)">
      <listOfVariables>
        <variable id="tim1_orig" taskReference="task1" target="/cellml:model/cellml:component[@cmeta:id='MT']/cellml:variable[@cmeta:id='MT_MT']" />
      </listOfVariables>
       <math:math>
          <math:apply>
            <math:divide />
            <math:ci>tim1_orig</math:ci>
            <math:apply>
              <math:csymbol encoding="text" definitionURL="http://sed-ml.org/#max">max</math:csymbol>
              <math:ci>tim1_orig</math:ci>
            </math:apply>
          </math:apply>
        </math:math>
    </dataGenerator>

    <dataGenerator id="per_tim" name="nuclear PER-TIM complex">
      <listOfVariables>
        <variable id="v1" taskReference="task1" target="/cellml:model/cellml:component[@cmeta:id='CN']/cellml:variable[@cmeta:id='CN_CN']" />
      </listOfVariables>
      <listOfParameters>
        <parameter id="p1" name="Zero" value="0" />
      </listOfParameters>
      <math:math>
        <math:apply>
          <math:plus />
          <math:ci>v1</math:ci>
          <math:ci>p1</math:ci>
        </math:apply>
      </math:math>
    </dataGenerator>
    
    <dataGenerator id="tim2" name="tim mRNA (changed parameters)">
      <listOfVariables>
        <variable id="v2" taskReference="task2" target="/cellml:model/cellml:component[@cmeta:id='MT']/cellml:variable[@cmeta:id='MT_MT']" />
      </listOfVariables>  
        <math:math>
          <math:ci>v2</math:ci>
        </math:math>
    </dataGenerator>
    
    <dataGenerator id="per_tim2" name="nuclear PER-TIM complex">
      <listOfVariables>
        <variable id="v3" taskReference="task2" target="/cellml:model/cellml:component[@cmeta:id='CN']/cellml:variable[@cmeta:id='CN_CN']" />
      </listOfVariables>
      <math:math>
        <math:apply>
          <math:plus />
          <math:ci>v3</math:ci>
        </math:apply>
      </math:math>
    </dataGenerator>
  </listOfDataGenerators>
  
  <listOfOutputs>
    <plot2D id="plot1" name="tim mRNA with Oscillation and Chaos (phase plot)">
      <listOfCurves>
        <curve id="c1" logX="false" logY="false" xDataReference="per_tim" yDataReference="tim1" />
        <curve id="c2" logX="false" logY="false" xDataReference="per_tim2" yDataReference="tim2" />
      </listOfCurves>
    </plot2D>
    <plot2D id="plot1" name="tim mRNA with Oscillation and Chaos (timecourse)">
      <listOfCurves>
        <curve id="c1t" logX="false" logY="false" xDataReference="time" yDataReference="tim1" />
        <curve id="c2t" logX="false" logY="false" xDataReference="time" yDataReference="tim2" />
        <curve id="c1t_norm" logX="false" logY="false" xDataReference="time" yDataReference="tim1_norm" />
      </listOfCurves>
    </plot2D>
  </listOfOutputs>
</sedML>
